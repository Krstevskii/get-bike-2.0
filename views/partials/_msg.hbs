{{#if error_msg}}
    {{#each error_msg}}
        <div class="container">
            <div class="flash-msg-err">
                <p><i class="fa fa-times"></i> {{this}}</p>
            </div>
        </div>
    {{/each}}
{{else}}
    {{#if success_msg}}
        <div class="container">
            <div class="flash-msg">
                <p><i class="fa fa-check"></i> {{success_msg}}</p>
            </div>
        </div>
    {{/if}}
{{/if}}
